/* 设置窗口背景颜色为深色 */
QMainWindow {
    background-color: #2B2B2B;  /* 深色背景 */
    color: #FFFFFF;             /* 白色字体 */
}

/* 按钮样式 */
QPushButton {
    background-color: #3E3E3E;  /* 深灰色背景 */
    border: 2px solid #666666;
    border-radius: 5px;
    padding: 10px;
    color: #FFFFFF;  /* 白色字体 */
}

QPushButton:hover {
    background-color: #4E4E4E;  /* 悬停时背景变浅 */
}

QPushButton:pressed {
    background-color: #5E5E5E;  /* 按下时背景更浅 */
}
